          <AuxValues>
            <AuxValue name="JavaCodeGenerator_AddingCodePost" type="java.lang.String" value="bodySearchButton.addActionListener(new ActionListener() { &#xa;    public void actionPerformed(ActionEvent e) { &#xa;        setStatusLabel(&quot;Running&quot;);&#xa;        refreshStatusPanel();&#xa;        String link = WebCrawler.gui.getURLField();&#xa;        if( !link.contains(&quot;https://&quot;)){&#xa;            link = &quot;https://&quot;+link;&#xa;        }&#xa;        String keyword = WebCrawler.gui.getKeywordField();&#xa;&#xa;        if( !keyword.isEmpty() &amp;&amp; getNoLinks() &gt;= 0 ){&#xa;            WebCrawler.linkCollector.setPhrase(keyword);&#xa;            WebCrawler.linkCollector.setLinksToCrawl(getNoLinks());&#xa;            if(!getOutputField().isEmpty()){&#xa;                WebCrawler.linkCollector.setOutputFile(getOutputField());&#xa;            }&#xa;            if( link.toLowerCase().contains(&quot;wikipedia.org&quot;)){&#xa;                WebCrawler.linkCollector.setWikiLang(link);&#xa;                WebCrawler.linkCollector.scrapWiki(link);&#xa;            } else {&#xa;                WebCrawler.linkCollector.searchByBody(link);&#xa;            } &#xa;            WebCrawler.linkCollector.writeToFile();&#xa;            WebCrawler.linkCollector.openFile();&#xa;        } else if( keyword.isEmpty()){&#xa;            out.println(&quot;Enter a keyword!&quot;);&#xa;        } else if( getNoLinks() &lt; 0 ){&#xa;            out.println(&quot;Number of links to crawl : bad data type! Enter integer value!&quot;);&#xa;        }&#xa;        setStatusLabel(&quot;Done&quot;);&#xa;        refreshStatusPanel();&#xa;    }      &#xa;    } &#xa;);"/>
          </AuxValues>
        </Component>
